    "description": "Generic library to integrate with rest APIs",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adinan-cenci/psr-18"
        }
    ],
    "authors": [
        {
            "name": "Adinan Cenci",
